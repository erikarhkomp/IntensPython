arr = [16, -5, -30, 60, 2, 0, -10, 1]
size = len(arr)

# Вывод неотрицательных элементов
print("Неотрицательные элементы массива:")
for i in range(size):
    if arr[i] >= 0:
        print(arr[i], end=" ")

# Вывод элементов, не превышающих 100
print("\nЭлементы массива, не превышающие 100:")
for i in range(size):
    if arr[i] <= 100:
        print(arr[i], end=" ")
